{
  "name": "@tech-frontier/ui-desktop",
  "version": "0.7.0",
  "main": "src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json",
    "./style.css": "./src/index.css"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "require": {
          "types": "./dist/index.d.ts",
          "default": "./dist/index.js"
        },
        "import": {
          "types": "./dist/index.d.mts",
          "default": "./dist/index.mjs"
        }
      },
      "./package.json": "./package.json",
      "./style.css": "./dist/index.css"
    },
    "main": "./dist/index.js"
  },
  "scripts": {
    "prepare": "yarn panda codegen",
    "prepack": "yarn build && node ../../.scripts/prePublish.js",
    "build": "yarn prepare && rm -rf dist && tsup ./src/index.ts --format cjs,esm --dts",
    "dev": "yarn run build --watch",
    "lint": "yarn prepare && lint",
    "typecheck": "yarn prepare && yarn tsc --noEmit",
    "storybook": "yarn prepare && storybook dev -p 6006",
    "build-storybook": "yarn prepare && storybook build"
  },
  "dependencies": {
    "eslint": "8.44.0",
    "eslint-config-next": "13.4.9",
    "react": "^18",
    "react-dom": "^18",
    "typescript": "5.1.6"
  },
  "devDependencies": {
    "@pandacss/dev": "^0.9.0",
    "@storybook/addon-essentials": "^7.0.6",
    "@storybook/addon-interactions": "^7.0.6",
    "@storybook/addon-links": "^7.0.6",
    "@storybook/addon-styling": "^1.3.6",
    "@storybook/blocks": "^7.0.6",
    "@storybook/jest": "^0.1.0",
    "@storybook/react": "^7.0.6",
    "@storybook/react-vite": "^7.0.6",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@swc/core": "^1.3.72",
    "@types/eslint": "^8",
    "@types/node": "20.4.1",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.44.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "13.4.9",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "latest",
    "msw": "^1.2.1",
    "msw-storybook-addon": "^1.8.0",
    "postcss": "^8.4.27",
    "prop-types": "^15.8.1",
    "storybook": "^7.0.6",
    "ts-node": "^10.9.1",
    "tsup": "^7.1.0",
    "typescript": "5.1.6",
    "vite": "^4.2.0"
  }
}
